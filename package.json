{
  "name": "io-project",
  "version": "1.0.0",
  "description": "frontend repository",
  "main": "index.js",
  "scripts": {
    "test": ".\"/node_modules/.bin/jest\" --watch",
    "test-debug": "node --inspect-brk \"node_modules/.bin/jest\" --runInBand --watch",
    "test-coverage": "\"node_modules/.bin/jest\" --coverage --watch",
    "test-watch": "jest src --watch",
    "test-update": "jest src --updateSnapshot",
    "start-server": "\"node_modules/.bin/ts-node\" --compiler typescript --compilerOptions '{\"target\": \"es5\", \"module\": \"commonjs\"}' \"server/index.ts\"",
    "watch:dev": "better-npm-run watch:dev",
    "watch:prod": "better-npm-run watch:prod",
    "build:test": "better-npm-run build:test",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "build:stage": "better-npm-run build:stage",
    "build:local": "better-npm-run build:local",
    "dev": "npm run watch:dev",
    "prod": "npm run watch:prod",
    "deploy": "better-npm-run build:gh-pages && better-npm-run deploy",
    "yeoman": "better-npm-run initial:yeoman",
    "update-backoffice-server-types": "./node_modules/.bin/sw2dts ./docs/backoffice.swagger.json > ./src/shared/types/server.ts; ./node_modules/.bin/tslint --fix ./src/shared/types/server.ts",
    "update-frontoffice-server-types": "./node_modules/.bin/sw2dts ./docs/frontoffice.swagger.json > ./src/shared/types/frontoffice_server.ts; ./node_modules/.bin/tslint --fix ./src/shared/types/frontoffice_server.ts"
  },
  "betterScripts": {
    "initial:yeoman": {
      "command": "sh ./generators/initializationGenerator.sh",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "watch:dev": {
      "command": "\"node_modules/.bin/webpack-dev-server\" --watch --progress --colors --config \"webpack/dev.config.ts\"",
      "env": {
        "NODE_ENV": "development",
        "WATCH_MODE": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}",
        "MOBILE": true
      }
    },
    "watch:prod": {
      "command": "\"node_modules/.bin/webpack-dev-server\" --client-log-level info --watch --progress --colors --config webpack/prod.config.ts",
      "env": {
        "NODE_ENV": "production",
        "WATCH_MODE": true,
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:dev": {
      "command": "\"node_modules/.bin/webpack\" --progress --colors --config \"webpack/dev.config.ts\"",
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:prod": {
      "command": "\"node_modules/.bin/webpack\" --progress --colors --config \"webpack/prod.config.ts\"",
      "env": {
        "NODE_ENV": "production",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}",
        "MOBILE": true
      }
    },
    "build:stage": {
      "command": "\"node_modules/.bin/webpack\" --progress --colors --config \"webpack/prod.config.ts\"",
      "env": {
        "NODE_ENV": "production",
        "CONTOUR": "stage",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:infra": {
      "command": "\"node_modules/.bin/webpack\" --progress --colors --config \"webpack/prod.config.ts\"",
      "env": {
        "NODE_ENV": "production",
        "CONTOUR": "infra",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:local": {
      "command": "\"node_modules/.bin/webpack\" --progress --colors --config \"webpack/prod.config.ts\"",
      "env": {
        "NODE_ENV": "production",
        "CONTOUR": "local",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    },
    "build:test": {
      "command": "\"node_modules/.bin/webpack\" --progress --colors --config \"webpack/prod.config.ts\"",
      "env": {
        "NODE_ENV": "production",
        "CONTOUR": "test",
        "TS_NODE_COMPILER_OPTIONS": "{\"target\": \"es5\", \"module\": \"commonjs\"}"
      }
    }
  },
